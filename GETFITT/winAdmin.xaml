<Window x:Class="GETFITT.winAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GETFITT"
        mc:Ignorable="d"
        Title="winAdmin" Height="450" Width="450">
    <Grid>
        <DataGrid x:Name="UsersDataGrid" HorizontalAlignment="Left" Height="287" Margin="10,10,0,0" VerticalAlignment="Top" Width="423"/>
        <StackPanel HorizontalAlignment="Left" Height="95" Margin="117,315,0,0" VerticalAlignment="Top" Width="206">
            <Button x:Name="btnSave" Content="Save" Height="34" Margin="0,0,0.4,0" FontSize="20" Click="btnSave_Click"/>
            <Label Content="" Height="12" Margin="0,0,0.4,0"/>
            <Button x:Name="btnExit" Content="Exit" Height="34" FontSize="20" Click="btnExit_Click" Margin="0,0,0.4,0"/>
        </StackPanel>
    </Grid>
</Window>
